generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  uuid        String     @id @default(uuid())
  createdAt   DateTime   @default(now())
  email       String     @unique
  name        String
  password    String
  rtHash      String?

  @@map("users")
}
