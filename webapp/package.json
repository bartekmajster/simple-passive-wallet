{
  "name": "funds-tracker-webapp",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check:circular-deps": "madge --warning --circular --extensions ts,tsx src",
    "check:circular-deps-image": "madge --image graph.svg --extensions ts,tsx src",
    "test": "jest --colors --runInBand --detectOpenHandles",
    "test:cov": "jest --colors --runInBand --detectOpenHandles --coverage --no-cache && open ./coverage/lcov-report/index.html",
    "test:badges": "pnpm test:cov && jest-coverage-badges",
    "test:watch": "jest --colors --runInBand --detectOpenHandles --watch",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "format": "prettier --write ./src",
    "codegen": "graphql-codegen --config ./src/config/codegen.ts",
    "codegen:watch": "graphql-codegen -w --config ./src/config/codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "3.7.7",
    "@ebay/nice-modal-react": "^1.2.8",
    "@graphql-codegen/client-preset": "2.0.0",
    "@hookform/resolvers": "2.9.11",
    "babel-plugin-styled-components": "2.0.7",
    "color2k": "2.0.2",
    "framer-motion": "9.0.2",
    "graphql": "16.6.0",
    "i18next": "22.4.9",
    "i18next-browser-languagedetector": "7.0.1",
    "i18next-http-backend": "2.1.1",
    "js-cookie": "3.0.1",
    "logrocket": "3.0.1",
    "react": "18.2.0",
    "react-currency-input-field": "3.6.10",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-google-recaptcha-v3": "1.10.1",
    "react-hook-form": "7.43.1",
    "react-i18next": "12.1.5",
    "react-icons": "4.7.1",
    "react-is": "18.2.0",
    "react-laag": "2.0.5",
    "react-router-dom": "6.8.1",
    "react-toastify": "9.1.1",
    "recharts": "2.4.1",
    "simple-flexbox": "2.3.3",
    "styled-components": "5.3.6",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/typed-document-node": "3.0.0",
    "@graphql-codegen/typescript": "3.0.0",
    "@graphql-codegen/typescript-operations": "3.0.0",
    "@graphql-typed-document-node/core": "3.1.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.4.0",
    "@types/js-cookie": "3.0.2",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/styled-components": "5.1.26",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "@vitejs/plugin-react": "3.1.0",
    "babel-loader": "9.1.2",
    "eslint": "8.33.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
