{
  "name": "funds-tracker-webapp",
  "version": "0.0.1",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "check:circular-deps": "madge --warning --circular --extensions ts,tsx src",
    "check:circular-deps-image": "madge --image graph.svg --extensions ts,tsx src",
    "test": "jest --colors --runInBand --detectOpenHandles --collectCoverage=false",
    "test:coverage": "jest --colors --runInBand --detectOpenHandles --coverage --no-cache",
    "test:badges": "pnpm test:coverage && jest-coverage-badges",
    "test:watch": "jest --colors --runInBand --detectOpenHandles --watch --collectCoverage=false",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "format": "prettier --write ./src"
  },
  "nx": {
    "targets": {
      "start": {
        "dependsOn": [
          "start"
        ]
      },
      "build": {
        "outputs": [
          "webapp/dist/"
        ],
        "dependsOn": [
          "build"
        ]
      },
      "lint": {
        "dependsOn": [
          "lint"
        ]
      },
      "test": {
        "dependsOn": [
          "test"
        ]
      }
    }
  },
  "dependencies": {
    "color2k": "2.0.0",
    "formik": "2.2.9",
    "i18next": "21.8.7",
    "i18next-browser-languagedetector": "6.1.4",
    "i18next-http-backend": "1.4.1",
    "jest-styled-components": "7.0.8",
    "js-cookie": "3.0.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-i18next": "11.16.11",
    "react-icons": "4.4.0",
    "react-laag": "2.0.4",
    "react-router-dom": "6.3.0",
    "simple-flexbox": "2.3.3",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@storybook/addon-a11y": "6.5.9",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-interactions": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/react": "6.5.9",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/react-hooks": "8.0.0",
    "@types/jest": "28.1.0",
    "@types/js-cookie": "3.0.2",
    "@types/react": "18.0.10",
    "@types/react-dom": "18.0.5",
    "@types/styled-components": "5.1.25",
    "@types/testing-library__jest-dom": "5.14.3",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "@vitejs/plugin-react": "1.3.2",
    "babel-loader": "8.2.5",
    "eslint": "8.16.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest-dom": "4.0.2",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-testing-library": "5.5.1",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "jest-watch-typeahead": "1.1.0",
    "madge": "5.0.1",
    "mq-polyfill": "1.1.8",
    "prettier": "2.6.2",
    "rollup-plugin-analyzer": "4.0.0",
    "storybook-addon-styled-component-theme": "2.0.0",
    "ts-jest": "28.0.4",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.7.2",
    "vite": "2.9.9",
    "vite-plugin-eslint": "1.6.1",
    "vite-plugin-static-copy": "0.5.1",
    "vite-plugin-svgr": "2.1.0",
    "vite-tsconfig-paths": "3.5.0"
  }
}
