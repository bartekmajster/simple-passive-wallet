{
	"name": "webapp",
	"scripts": {
		"build": "tsc && vite build",
		"clean": "rm -rf dist",
		"dev": "vite --clearScreen false",
		"check:circular-deps": "madge --warning --circular --extensions ts,tsx src",
		"check:circular-deps-image": "madge --image graph.svg --extensions ts,tsx src",
		"test": "vitest run",
		"test:cov": "vitest run --coverage && open ./coverage/index.html",
		"test:badges": "pnpm test:cov && jest-coverage-badges",
		"test:watch": "vitest",
		"lint": "eslint src",
		"lint:fix": "eslint src --fix",
		"codegen": "graphql-codegen --config ./src/config/codegen.ts",
		"codegen:watch": "graphql-codegen -w --config ./src/config/codegen.ts",
		"generate-pwa-assets": "pwa-assets-generator"
	},
	"dependencies": {
		"@apollo/client": "3.7.17",
		"@ebay/nice-modal-react": "1.2.10",
		"@graphql-codegen/client-preset": "4.0.1",
		"@hookform/resolvers": "3.1.1",
		"@types/react-datepicker": "4.11.2",
		"clsx": "^2.0.0",
		"color2k": "2.0.2",
		"date-fns": "2.30.0",
		"framer-motion": "10.12.18",
		"graphql": "16.7.1",
		"i18next": "23.2.8",
		"i18next-browser-languagedetector": "7.1.0",
		"i18next-http-backend": "2.2.1",
		"js-cookie": "3.0.5",
		"logrocket": "4.0.3",
		"next-themes": "^0.2.1",
		"react": "18.2.0",
		"react-currency-input-field": "3.6.11",
		"react-datepicker": "4.15.0",
		"react-dom": "18.2.0",
		"react-error-boundary": "4.0.10",
		"react-focus-lock": "2.9.5",
		"react-google-recaptcha-v3": "1.10.1",
		"react-hook-form": "7.45.1",
		"react-i18next": "13.0.1",
		"react-icons": "4.10.1",
		"react-is": "18.2.0",
		"react-laag": "2.0.5",
		"react-router-dom": "6.14.1",
		"react-toastify": "9.1.3",
		"styled-components": "6.0.5",
		"supertokens-web-js": "^0.8.0",
		"ui": "workspace:*",
		"yup": "1.2.0"
	},
	"devDependencies": {
		"@babel/core": "7.22.8",
		"@graphql-codegen/cli": "4.0.1",
		"@graphql-codegen/typed-document-node": "5.0.1",
		"@graphql-codegen/typescript": "4.0.1",
		"@graphql-codegen/typescript-operations": "4.0.1",
		"@graphql-typed-document-node/core": "3.2.0",
		"@hookform/devtools": "4.3.1",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/react": "14.0.0",
		"@testing-library/react-hooks": "8.0.1",
		"@testing-library/user-event": "14.4.3",
		"@types/js-cookie": "3.0.3",
		"@types/node": "20.4.1",
		"@types/react": "18.2.14",
		"@types/react-dom": "18.2.6",
		"@types/testing-library__jest-dom": "5.14.7",
		"@vite-pwa/assets-generator": "0.0.3",
		"@vitejs/plugin-react": "4.0.2",
		"@vitest/coverage-c8": "0.33.0",
		"autoprefixer": "^10.4.16",
		"babel-loader": "9.1.3",
		"babel-plugin-styled-components": "2.1.4",
		"c8": "8.0.0",
		"eslint-config-custom": "workspace:*",
		"jest-styled-components": "7.1.1",
		"jsdom": "22.1.0",
		"madge": "6.1.0",
		"mq-polyfill": "1.1.8",
		"postcss": "^8.4.31",
		"resize-observer-polyfill": "1.5.1",
		"tailwindcss": "3.3.3",
		"tsconfig": "workspace:*",
		"tsconfig-paths-webpack-plugin": "4.0.1",
		"typescript": "5.2.2",
		"vite": "4.4.2",
		"vite-plugin-eslint": "1.8.1",
		"vite-plugin-pwa": "0.16.4",
		"vite-plugin-static-copy": "0.16.0",
		"vite-plugin-svgr": "3.2.0",
		"vite-tsconfig-paths": "4.2.0",
		"vitest": "0.33.0",
		"workbox-window": "7.0.0"
	}
}
