{
	"name": "webapp",
	"type": "module",
	"scripts": {
		"build": "tsc && vite build",
		"clean": "rm -rf dist",
		"dev": "vite --clearScreen false",
		"check:circular-deps": "madge --warning --circular --extensions ts,tsx src",
		"check:circular-deps-image": "madge --image graph.svg --extensions ts,tsx src",
		"test": "vitest run",
		"test:webapp": "pnpm test",
		"test:cov": "vitest run --coverage && open ./coverage/index.html",
		"test:badges": "pnpm test:cov && jest-coverage-badges",
		"test:watch": "vitest",
		"lint": "eslint src",
		"lint:fix": "eslint src --fix",
		"codegen": "graphql-codegen --config ./src/config/codegen.ts",
		"codegen:watch": "graphql-codegen -w --config ./src/config/codegen.ts",
		"generate-pwa-assets": "pwa-assets-generator",
		"tsc": "tsc"
	},
	"dependencies": {
		"@apollo/client": "3.8.8",
		"@graphql-codegen/client-preset": "4.1.0",
		"@hookform/resolvers": "3.3.4",
		"@internationalized/date": "3.5.1",
		"clsx": "2.1.0",
		"framer-motion": "10.17.9",
		"graphql": "16.8.1",
		"i18next": "23.7.16",
		"i18next-browser-languagedetector": "7.2.0",
		"i18next-http-backend": "2.4.2",
		"js-cookie": "3.0.5",
		"logrocket": "7.0.0",
		"lucide-react": "0.305.0",
		"next-themes": "0.2.1",
		"react": "18.2.0",
		"react-aria": "3.31.0",
		"react-currency-input-field": "3.6.14",
		"react-dom": "18.2.0",
		"react-error-boundary": "4.0.12",
		"react-focus-lock": "2.9.6",
		"react-google-recaptcha-v3": "1.10.1",
		"react-hook-form": "7.49.2",
		"react-i18next": "14.0.0",
		"react-is": "18.2.0",
		"react-laag": "2.0.5",
		"react-router-dom": "6.21.1",
		"react-stately": "3.29.0",
		"react-toastify": "9.1.3",
		"@faunds-tracker/ui": "workspace:*",
		"yup": "1.3.3"
	},
	"devDependencies": {
		"@babel/core": "7.23.7",
		"@graphql-codegen/cli": "5.0.0",
		"@graphql-codegen/typed-document-node": "5.0.1",
		"@graphql-codegen/typescript": "4.0.1",
		"@graphql-codegen/typescript-operations": "4.0.1",
		"@graphql-typed-document-node/core": "3.2.0",
		"@hookform/devtools": "4.3.1",
		"@testing-library/jest-dom": "6.2.0",
		"@testing-library/react": "14.1.2",
		"@testing-library/react-hooks": "8.0.1",
		"@testing-library/user-event": "14.5.2",
		"@types/js-cookie": "3.0.6",
		"@types/node": "20.10.6",
		"@types/react": "18.2.46",
		"@types/react-dom": "18.2.18",
		"@types/testing-library__jest-dom": "6.0.0",
		"@vite-pwa/assets-generator": "0.2.2",
		"@vitejs/plugin-react": "4.2.1",
		"@vitest/coverage-c8": "0.33.0",
		"autoprefixer": "10.4.16",
		"eslint-config-custom": "workspace:*",
		"jsdom": "23.1.0",
		"madge": "6.1.0",
		"mq-polyfill": "1.1.8",
		"postcss": "8.4.33",
		"resize-observer-polyfill": "1.5.1",
		"tailwindcss": "3.4.0",
		"tsconfig": "workspace:*",
		"tsconfig-paths-webpack-plugin": "4.1.0",
		"typescript": "5.3.3",
		"vite": "5.0.11",
		"vite-plugin-eslint": "1.8.1",
		"vite-plugin-pwa": "0.17.4",
		"vite-plugin-svgr": "4.2.0",
		"vite-tsconfig-paths": "4.2.3",
		"vitest": "1.1.3",
		"workbox-window": "7.0.0"
	}
}
