{
  "name": "funds-tracker-webserver",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "check:circular-deps": "madge --warning --circular --extensions ts,tsx src",
    "check:circular-deps-image": "madge --image graph.svg --extensions ts,tsx src",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "dotenv -e .env.test npx prisma migrate deploy && dotenv -e .env.test jest",
    "test:ci": "npx prisma migrate deploy && jest",
    "test:cov": "dotenv -e .env.test npx prisma migrate deploy && dotenv -e .env.test -- jest --coverage && open ./coverage/lcov-report/index.html",
    "test:watch": "dotenv -e .env.test npx prisma migrate deploy && dotenv -e .env.test -- jest --watch"
  },
  "dependencies": {
    "@apollo/server": "4.5.0",
    "@graphql-tools/utils": "9.2.1",
    "@nestjs/apollo": "11.0.3",
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "9.3.10",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.3.10",
    "@nestjs/graphql": "11.0.0",
    "@nestjs/jwt": "10.0.2",
    "@nestjs/mapped-types": "1.2.2",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.3.10",
    "@prisma/client": "4.11.0",
    "@sendgrid/mail": "7.7.0",
    "apollo-server-core": "3.12.0",
    "apollo-server-express": "3.12.0",
    "bcrypt": "5.1.0",
    "cache-manager": "5.1.7",
    "cache-manager-redis-store": "3.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "1.4.6",
    "graphql": "16.6.0",
    "graphql-scalars": "1.20.4",
    "handlebars": "4.7.7",
    "nodemailer": "6.9.1",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.4.0",
    "rxjs": "7.8.0",
    "ua-parser-js": "1.0.34"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "0.3.5",
    "@nestjs/cli": "9.2.0",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.10",
    "@types/bcrypt": "5.0.0",
    "@types/cache-manager": "4.0.2",
    "@types/cache-manager-redis-store": "2.0.1",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.17",
    "@types/jest": "29.2.5",
    "@types/node": "18.15.3",
    "@types/nodemailer": "6.4.7",
    "@types/passport-jwt": "3.0.8",
    "@types/supertest": "2.0.12",
    "@types/ua-parser-js": "0.7.36",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "eslint": "8.36.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
