{
  "name": "funds-tracker-webserver",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "check:circular-deps": "madge --warning --circular --extensions ts,tsx src",
    "check:circular-deps-image": "madge --image graph.svg --extensions ts,tsx src",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "dotenv -e .env.test npx prisma migrate deploy && dotenv -e .env.test jest",
    "test:ci": "npx prisma migrate deploy && jest",
    "test:cov": "dotenv -e .env.test npx prisma migrate deploy && dotenv -e .env.test -- jest --coverage && open ./coverage/lcov-report/index.html",
    "test:watch": "dotenv -e .env.test npx prisma migrate deploy && dotenv -e .env.test -- jest --watch"
  },
  "dependencies": {
    "@graphql-tools/utils": "9.1.1",
    "@nestjs/apollo": "10.1.6",
    "@nestjs/axios": "0.1.0",
    "@nestjs/common": "9.1.5",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.1.5",
    "@nestjs/graphql": "10.1.6",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/mapped-types": "1.2.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.1.5",
    "@prisma/client": "4.5.0",
    "@types/ua-parser-js": "^0.7.36",
    "apollo-server-core": "3.11.1",
    "apollo-server-express": "3.11.1",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cookie-parser": "1.4.6",
    "graphql": "16.6.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.7",
    "ua-parser-js": "^1.0.32"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "0.3.3",
    "@nestjs/cli": "9.1.4",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.1.5",
    "@types/bcrypt": "5.0.0",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.0",
    "@types/node": "18.11.3",
    "@types/passport-jwt": "3.0.7",
    "@types/sinon": "10.0.13",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "eslint": "8.26.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "graphql-tag": "2.12.6",
    "jest": "29.2.1",
    "jest-watch-typeahead": "^2.2.1",
    "madge": "5.0.1",
    "prettier": "2.7.1",
    "prisma": "4.5.0",
    "sinon": "15.0.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.0",
    "supertest-graphql": "1.1.4",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-morph": "17.0.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4",
    "webpack": "5.74.0"
  }
}
