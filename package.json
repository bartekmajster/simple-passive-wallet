{
	"private": true,
	"scripts": {
		"prepare": "docker compose up -d && pnpm prisma:generate && pnpm prisma:migrate-dev",
		"prisma:generate": "pnpm dlx prisma generate",
		"prisma:migrate-dev": "pnpm dlx prisma migrate dev",
		"prisma:studio": "pnpm dlx prisma studio",
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"lint:fix": "turbo run lint:fix",
		"format": "prettier --write \"**/*.{js,ts,tsx,md}\""
	},
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"devDependencies": {
		"eslint": "8.51.0",
		"prettier": "3.0.3",
		"tsconfig": "workspace:*",
		"turbo": "1.10.15"
	},
	"packageManager": "pnpm@8.6.10",
	"name": "funds-tracker"
}
